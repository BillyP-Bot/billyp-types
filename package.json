{
	"name": "btbot-types",
	"version": "1.1.0",
	"description": "a shared types package for billy suite",
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"engines": {
		"node": "^14.15.0 || ^16.6.2",
		"yarn": "^1.22.5"
	},
	"scripts": {
		"prepare": "husky install",
		"lint": "eslint --fix --ext .ts .",
		"clean": "npx rimraf ./yarn.lock && npx rimraf ./lib && npx rimraf ./**/node_modules",
		"build": "tsc",
		"pack:light": "yarn build && npm pack --dry-run",
		"pack:release": "yarn build && npm pack",
		"release": "yarn build && npm publish"
	},
	"author": "Jude Giordano",
	"license": "MIT",
	"devDependencies": {
		"@types/chai": "^4.3.0",
		"@types/jest": "^27.0.3",
		"@types/mocha": "^9.0.0",
		"@types/node": "^17.0.2",
		"@typescript-eslint/eslint-plugin": "^5.25.0",
		"@typescript-eslint/parser": "^4.10.0",
		"chai": "^4.3.4",
		"eslint": "^7.16.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.0.0",
		"husky": "^7.0.0",
		"jest": "^27.3.1",
		"mocha": "^9.1.3",
		"prettier": "^2.6.2",
		"ts-jest": "^27.0.7",
		"ts-node": "^10.4.0",
		"typescript": "^4.5.2"
	}
}
